/* pages/tabbar/cart/success/success.wxss */
.result{
    display: flex;
    flex-flow: column;
    align-items: center;
    padding: 100rpx 50rpx;
}
.result .icon{
    padding-bottom: 40rpx;
}
.result .title{
    font-size: 40rpx;
}
.result .label{
    font-size: 26rpx;
    padding-top: 20rpx;
    color: #666666;
}
.result .button{
    width: 100%;
    display: flex;
    flex-flow: column;
    padding: 50rpx 0;
}
.result .sub-btn{
    width: 100%;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    font-size: 32rpx;
    background: #33cd5f;
    color: #ffffff;
    margin-bottom: 30rpx;
    border-radius: 10rpx;
}
.result .sub-btn:last-child{
    color: #333333;
    background: white;
}
