.list{
    margin-top: 10rpx;
}

.list .item {
    background: #fff;
    margin-left: 20rpx;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 25rpx;
    padding-right: 0;
    border-radius: 10rpx;
}

.list .item>text:last-of-type {
    color: #FF8A00;
    background: #FFEED3;
    padding: 6rpx 25rpx;
    font-size: 22rpx;
    border-top-left-radius: 20px;
    border-bottom-left-radius: 20px;
    display: flex;
    align-items: center;
}

.list .item .price {
    font-size: 28rpx;
    font-weight: 500;
}

.empty{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    margin-top: 300rpx;
    color: #999999
}