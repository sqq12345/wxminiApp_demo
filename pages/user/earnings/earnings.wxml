<view class="container">
    <view class="list" wx:if="{{list.length>0}}">
        <view class="item" wx:for="{{list}}" wx:key="{{index}}">
            <text>订单号:{{item.orderNo}}</text>
            <text>分成金额:￥<text class="price">{{item.money}}</text></text>
        </view>
    </view>
    <view class="empty" wx:else>
        暂无收益
    </view>
</view>
